{
  "name": "@hot-updater/console",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "main": "./dist/main.mjs",
  "scripts": {
    "build": "rsbuild build --mode production",
    "check": "biome check --write",
    "dev": "rsbuild dev --open",
    "format": "biome format --write",
    "preview": "rsbuild preview",
    "dev:gui": "concurrently \"cross-env NODE_ENV=development rsbuild dev\" \"wait-on tcp:3000 && cross-env NODE_ENV=development electron main.js\"",
